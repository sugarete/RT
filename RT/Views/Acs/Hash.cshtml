@model RT.Models.ViewModels.AcsVM

@{
    ViewBag.Title = "HashCodeRecovery";
    Layout = "~/Views/Shared/_LayoutPage2.cshtml";
}

@using (Html.BeginForm("Hash", "Acs", FormMethod.Post))
{
    <div class="input-group row w-75 mt-3 d-flex justify-content-center align-items-center">
        @Html.LabelFor(m => m.Hash, "Hash", new { @class = "col-2 text-center" })
        @Html.TextBoxFor(m => m.Hash, "Hash", new { placeholder = "Please Enter Hash", @class = "form-control col-10" })
    </div>
    <div class="input-group row w-75 mt-3 d-flex justify-content-center align-items-center">
        @Html.LabelFor(m => m.HashType, "HashType", new { @class = "col-2 text-center" })
        @Html.TextBoxFor(m => m.HashType, "HashType", new { placeholder = "Please Enter HashType", @class = "form-control col-10" })
    </div>
    <div class="input-group row w-75 mt-3 d-flex justify-content-center align-items-center">
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        <button class="btn btn-primary mt-3 w-50 col-10" type="submit">AttackSetting</button>
    </div>
}




